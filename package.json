{
  "dependencies": {
    "file-saver": "^2.0.1",
    "ol": "^5.3.1",
    "papaparse": "4.6.2",
    "resumablejs": "^1.1.0",
    "sidebar-v2": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.2.0",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "css-loader": "^0.28.11",
    "eslint": "^5.14.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "scripts": {
    "build": "npm run build:css & npm run build:js",
    "build:css": "node-sass --output-style compressed --output public/css resources/sass/style.scss",
    "build:js": "webpack --config webpack.config.js --mode production --progress",
    "test": "eslint resources/javascript/",
    "fix": "eslint resources/javascript/ --fix",
    "watch": "npm run watch:css & npm run watch:js",
    "watch:css": "node-sass --watch --output-style compressed --output public/css resources/sass/style.scss",
    "watch:js": "webpack --watch --config webpack.config.js --mode development --progress"
  }
}
