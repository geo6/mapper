{
    "name": "geo6/mapper",
    "type": "project",
    "description": "Display (and query) geographical data (WMS, WMTS, GeoJSON, KML, GPX).",
    "keywords": [
        "mezzio",
        "zend-framework",
        "zend",
        "mapper",
        "wms",
        "wmts",
        "geojson",
        "kml",
        "gpx",
        "csv",
        "viewer",
        "geocoder",
        "expressive"
    ],
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "geo6/geocoder-php-bpost-provider": "1.2.1",
        "geo6/geocoder-php-geo6-poi-provider": "2.0.0",
        "geo6/geocoder-php-geo6-provider": "2.1.0",
        "geo6/geocoder-php-geopunt-provider": "1.1.0",
        "geo6/geocoder-php-spw-provider": "1.2.0",
        "geo6/geocoder-php-urbis-provider": "1.1.0",
        "geo6/zend-expressive-monolog": "1.0.0",
        "geocoder-php/nominatim-provider": "5.2.0",
        "guzzlehttp/guzzle": "6.5.5",
        "intervention/image": "2.5.1",
        "laminas/laminas-component-installer": "2.3.2",
        "laminas/laminas-config": "3.4.0",
        "laminas/laminas-config-aggregator": "1.3.0",
        "laminas/laminas-dependency-plugin": "2.0.0beta1",
        "laminas/laminas-diactoros": "2.4.1",
        "laminas/laminas-servicemanager": "3.4.1",
        "laminas/laminas-stdlib": "3.3.0",
        "mezzio/mezzio": "3.2.2",
        "mezzio/mezzio-authentication-session": "1.0.1",
        "mezzio/mezzio-fastroute": "3.0.3",
        "mezzio/mezzio-helpers": "5.3.0",
        "mezzio/mezzio-session-ext": "1.10.0",
        "mezzio/mezzio-twigrenderer": "2.6.1",
        "mtymek/blast-base-url": "1.0.0",
        "php-http/guzzle6-adapter": "2.0.1",
        "php-http/message": "1.9.1",
        "sentry/sdk": "2.2.0"
    },
    "require-dev": {
        "ext-mbstring": "*",
        "filp/whoops": "^2.1",
        "laminas/laminas-development-mode": "^3.1",
        "mezzio/mezzio-tooling": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "phpunit/phpunit": "^9.1",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "suggest": {
        "ext-xml": "Needed to support XML configuration files.",
        "ext-yaml": "Needed to support YAML configuration files."
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "analyze": "phpstan analyze -l max -c ./phpstan.neon ./src ./config",
        "check": [
            "@cs-check",
            "@analyze"
        ],
        "clear-config-cache": "php bin/clear-config-cache.php",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "laminas-development-mode disable",
        "development-enable": "laminas-development-mode enable",
        "development-status": "laminas-development-mode status",
        "expressive": "expressive --ansi",
        "serve": "php -S localhost:8080 -t public/ -d display_errors=1",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    },
    "support": {
        "issues": "https://github.com/geo6/mapper/issues",
        "source": "https://github.com/geo6/mapper",
        "rss": "https://github.com/geo6/mapper/releases.atom"
    }
}
